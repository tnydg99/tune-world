# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'TuneWorld' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TuneWorld
pod 'Alamofire', '~> 4.0'
pod 'MarqueeLabel/Swift'
pod 'TwitterKit'


  target 'TuneWorldTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire', '~> 4.0'
  end

  target 'TuneWorldUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
